{
    "name": "ceddyg/clara-entity-generator",
    "description": "Repository to generate a CRUD system (Controller, model, repository, view using bootstrap, and more) scanning the shema of the database.",
    "keywords": ["laravel", "clara", "crud", "entity"],
    "authors": [
        {
            "name": "CeddyG"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "CeddyG\\ClaraEntityGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CeddyG\\ClaraEntityGenerator\\EntityGeneratorServiceProvider"
            ]
        }
    }
}
